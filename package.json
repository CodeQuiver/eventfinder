{
  "name": "eventfinder",
  "version": "1.0.0",
  "description": "Event search app combining weather predictions, event search suggestions, and database to save events for later reference. Uses MERN stack.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
